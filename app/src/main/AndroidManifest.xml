<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.edu24">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".JoinActivity"
            android:theme="@style/Theme.MyApp"></activity>
        <activity
            android:name=".CreateAClassActivity"
            android:theme="@style/Theme.MyApp" />
        <activity android:name=".TermsActivity" />
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ClassRoomActivity" />
        <activity android:name=".ClassActivity" />
        <activity android:name=".AccountActivity" />
    </application>

</manifest>